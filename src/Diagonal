fun main() {
    val scanner = Scanner(System."in")

    val a = scanner.nextInt()
    val b = scanner.nextInt()
    val y = scanner.nextDouble()

    // TODO solution
    // sqrt(a2+b2 - 2abcosy)
    val rad = Math.toRadians(y)
    val res = sqrt(a*a + b*b - 2 * a * b * cos(rad))

    println("Result is $res")
